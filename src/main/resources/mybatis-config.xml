	<mappers>
		<mapper resource="mappers/memberMapper.xml" />
		<mapper resource="mappers/mainProjectMapper.xml" />
		<mapper resource="mappers/projectMemberMapper.xml" />
		<mapper resource="mappers/workSpaceMapper.xml" />
