<configuration>

	<typeAliases>
		<typeAlias type="com.team.sprout.vo.Member" alias="Member" />
		<typeAlias type="com.team.sprout.vo.MainProject" alias="MainProject" />
		<typeAlias type="com.team.sprout.vo.ProjectMember" alias="ProjectMember" />
